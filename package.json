{
	"name": "application",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm test:integration && pnpm test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"codegen:component": "tsx scripts/codegen-component.ts",
		"codegen:route": "tsx scripts/codegen-route.ts",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview",
		"test:coverage": "vitest run --coverage",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"typecheck": "tsc",
		"verify": "pnpm lint && pnpm typecheck && pnpm check"
	},
	"devDependencies": {
		"@csstools/postcss-global-data": "^3.0.0",
		"@fontsource-variable/sora": "^5.1.0",
		"@histoire/plugin-svelte": "^0.17.17",
		"@iconify/svelte": "^4.0.2",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/svelte": "^5.2.3",
		"@testing-library/user-event": "^14.5.2",
		"@types/eslint": "^9.6.0",
		"@types/jest": "^29.5.13",
		"@types/node": "^22.7.5",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"histoire": "^0.17.17",
		"modern-normalize": "^3.0.1",
		"postcss": "^8.4.47",
		"postcss-preset-env": "^10.0.7",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"tsx": "^4.19.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"unplugin-icons": "^0.19.3",
		"vite": "^5.0.3",
		"vitest": "^2.0.0"
	},
	"type": "module"
}
